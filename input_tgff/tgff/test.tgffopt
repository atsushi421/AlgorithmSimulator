# 乱数生成の種
seed 5

# 生成するDAGの数
tg_cnt 1

# DAGの最小タスク数。 x y → xから+-yの範囲で変動する
task_cnt 100 1

# ノードに入力されるエッジの最大数と出力するエッジの最大数を設定
task_degree 5 3

# new algorithmを使用
#gen_series_parallel true

# 直列チェーンの長さ（平均値、乗数）を設定。これは1で良い気がする
#series_len 1 0

# 直列チェーンの幅（平均値、乗数）を設定。幅は3以上ないと細くなる。
#series_wid 5 0

# 出口ノードを1つにする
#series_must_rejoin true

# 追加するローカルアークの数を設定。詳細不明
#series_local_xover 30

# 追加するグローバルアークの数を設定。詳細不明
#series_global_xover 30

# DAGをtgffファイルに出力
tg_write

# PEの情報をtgffファイルに出力
pe_write

# 通信時間をtgffファイルに出力
trans_write

# epsファイルを出力
eps_write

# vcgファイルを出力
vcg_write